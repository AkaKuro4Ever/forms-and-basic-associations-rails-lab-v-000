  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <br />
  <%= f.label :notes %>
  <input name="song[notes][]" name="song[notes][]">
  <br />
  <%= f.label :notes %>
  <input name="song[notes][]">
  <br />
  <%= f.label :notes %>
  <input name="song[notes][]">
  <br />

  <%= f.submit "commit"%>
<% end %>
